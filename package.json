{
  "name": "@1j1s/main-cms",
  "private": true,
  "version": "1.1.1",
  "description": "1 jeune 1 solution main CMS ",
  "scripts": {
    "prepare": "./setup.sh",
    "docker:start": "docker-compose --env-file=.env.docker up --build -d && sleep 5 && open http://localhost/admin",
    "docker:new-component": "docker-compose stop && docker-compose --env-file=.env.docker up --build -d",
    "docker:start::log": "docker-compose --env-file=.env.docker up --build -d && docker-compose logs -f",
    "docker:down": "docker-compose --env-file=.env.docker down",
    "docker:clean": "docker-compose --env-file=.env.docker down && docker volume prune",
    "docker:stop": "docker-compose --env-file=.env.docker stop",
    "develop": "strapi develop",
    "dev": "strapi develop",
    "start": "strapi start",
    "build": "strapi build",
    "strapi": "strapi",
    "test": "mocha"
  },
  "dependencies": {
    "@strapi/plugin-documentation": "~4.5.6",
    "@strapi/plugin-sentry": "~4.5.6",
    "@strapi/plugin-users-permissions": "~4.5.6",
    "@strapi/provider-upload-aws-s3": "~4.5.6",
    "@strapi/strapi": "4.5.6",
    "pg": "8.8.0",
    "strapi-plugin-config-sync": "~1.1.0",
    "strapi-plugin-import-export-entries": "~1.18.0",
    "strapi-plugin-meilisearch": "~0.9.1",
    "strapi-plugin-populate-deep": "~2.0.0",
    "strapi-plugin-slugify": "~2.3.2"
  },
  "devDependencies": {
    "@types/mocha": "~10.0.1",
    "@types/node": "~18.11.18",
    "git-notify": "~0.2.3",
    "mocha": "~10.2.0",
    "ts-node": "~10.9.1"
  },
  "author": {
    "name": "OCTO Technology"
  },
